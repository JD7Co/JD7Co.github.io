    logger.info(f"‚úÖ –í—Ö–æ–¥ –≤ —Å–∏—Å—Ç–µ–º—É: {message.from_user.id}")
    
    # –°–æ–∑–¥–∞–µ–º –∫–Ω–æ–ø–∫–∏
    builder = InlineKeyboardBuilder()

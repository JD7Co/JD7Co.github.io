from aiogram.utils.keyboard import InlineKeyboardBuilder

@dp.message(Command("start"))
async def cmd_start(message: types.Message):
    logger.info(f"‚úÖ –í—Ö–æ–¥ –≤ —Å–∏—Å—Ç–µ–º—É: {message.from_user.id}")
    
    # –°–æ–∑–¥–∞–µ–º –∫–Ω–æ–ø–∫–∏
    builder = InlineKeyboardBuilder()
    builder.row(types.InlineKeyboardButton(text="üìà VIP Trading", callback_data="trade"))
    builder.row(types.InlineKeyboardButton(text="üéµ Suno AI Hub", callback_data="suno"))
    builder.row(types.InlineKeyboardButton(text="üí∞ Passive Income", url="https://pawns.app/?r=1139563"))
    builder.row(types.InlineKeyboardButton(text="üõ° –°—Ç–∞—Ç—É—Å –°–∏—Å—Ç–µ–º—ã", callback_data="status_check"))

    await message.answer(
        f"üëã **–ü—Ä–∏–≤–µ—Ç—Å—Ç–≤—É—é, –î–º–∏—Ç—Ä–∏–π!**\n\n"
        f"–≠—Ç–æ —Ü–µ–Ω—Ç—Ä —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è **JDmitrijs7¬Æ**.\n"
        f"–í—Å–µ —Å–∏—Å—Ç–µ–º—ã —Ä–∞–±–æ—Ç–∞—é—Ç –≤ —à—Ç–∞—Ç–Ω–æ–º —Ä–µ–∂–∏–º–µ.\n"
        f"–í—ã–±–µ—Ä–∏—Ç–µ –Ω—É–∂–Ω—ã–π —Å–µ–∫—Ç–æ—Ä:",
        reply_markup=builder.as_markup()
  ) 

ADMIN_ID = 5666795893 # –ó–∞–º–µ–Ω–∏ –Ω–∞ —Å–≤–æ–π —Ä–µ–∞–ª—å–Ω—ã–π ID (–º–æ–∂–Ω–æ —É–∑–Ω–∞—Ç—å —É @userinfobot)

@dp.message(Command("start"))
async def cmd_start(message: types.Message):
    # –£–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ –¥–ª—è —Ç–µ–±—è
    if message.from_user.id != ADMIN_ID:
        await bot.send_message(
            ADMIN_ID, 
            f"üîî **–ù–æ–≤—ã–π –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å –≤ —Å–∏—Å—Ç–µ–º–µ!**\n–ò–º—è: {message.from_user.full_name}\nID: `{message.from_user.id}`"
        )
    
    # –û—Ç–≤–µ—Ç –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—é (—Ç–≤–æ–µ –º–µ–Ω—é)
    await message.answer("ü¶æ **JED –°–ò–°–¢–ï–ú–ê –ê–ö–¢–ò–í–ò–†–û–í–ê–ù–ê!**\n\n–í—ã–±–µ—Ä–∏—Ç–µ —Å–µ–∫—Ç–æ—Ä:")
